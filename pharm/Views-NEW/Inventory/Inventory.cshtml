@{
    ViewBag.Title = "Inventory";
    if (Session["Usertype"] != null)
    {
        if (Session["Usertype"].ToString() == "Admin")
        {
            Layout = "~/Views/Shared/_Layout.cshtml";
        }
        else
        {
            Layout = "~/Views/Shared/_LayoutStaff.cshtml";
        }
    }
    else
    {
        Response.Write("<script>alert('You dont have permission to access this page') ; window.location.href='/Login/Login'</script>");
    }
}

<h2>Inventory</h2>

<h3>@Html.ActionLink("Stock", "Index", "Stock")</h3>
<h3>@Html.ActionLink("Medicine", "Index", "Medicine")</h3>
<h3>@Html.ActionLink("Drug Generic Name", "Index", "DrugGenericName")</h3>
